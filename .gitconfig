[user]
  name = satoshun00
  email = shun.sato@fenneclab.com
[core]
  excludesfile = ~/.gitignore
  editor = code -w
[filter "media"]
  clean = git-media-clean %f
  smudge = git-media-smudge %f
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold # line info
  old = red # deletions
  new = green # additions
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[alias]
  st = status
  br = branch
  co = checkout
